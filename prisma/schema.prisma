// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Test model user for login, must delete this model before start developing.
model User {
  id       String @id @default(uuid())
  username String
  password String
}

model CrudSample {
  id         String  @id @default(uuid())
  testField1 String
  testField2 Int
  testField3 Boolean
}
